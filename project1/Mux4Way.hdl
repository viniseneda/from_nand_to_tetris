/**
 * 4-way multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way {
    IN a, b, c, d, sel[2];
    OUT out;

    PARTS:
    Mux(a=a, b=c, sel=sel[1], out=out1);
    Mux(a=b, b=d, sel=sel[1], out=out2);
    Mux(a=out1, b=out2, sel=sel[0], out=out);	  
}
